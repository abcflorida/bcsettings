{
	"name": "abcflorida/bcsettings",
	"type": "platform-extension",
	"description": "extend core settings",
        "version": "1.0.0",
	"authors": [
		{
			"name": "Brian Clinton",
			"email": "flbrianc@gmail.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.2.*",
		"illuminate/support": "~5.0",
		"platform/foundation": "~2.0|~3.0"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Abcflorida\\Bcsettings\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
